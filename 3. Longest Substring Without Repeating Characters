class Solution(object):
    def lengthOfLongestSubstring(self,s):
        results = []
        dict = {}
        begin = 0
        if (len(s)>0):
            if not any (m.strip()for m in s) or len(s)==1:
                    return 1
            else:        
                for i in range(len(s)):
                    if s[i]in dict:
                        begin = max(dict[s[i]],begin)
                        results.append(i-begin+1)

                    results.append(i-begin+1)
                    dict[s[i]]=i+1

                return(max(results))    
        else:
            return 0
